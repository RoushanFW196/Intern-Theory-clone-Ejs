



<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Login | Intern Theory</title>
    <link rel="stylesheet" href="/css/loginform.css">
    <script src="https://code.jquery.com/jquery-3.5.0.js"></script>
    <link rel="stylesheet" href="/css/navbar.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" />
    <link rel="stylesheet" href="/css/temp.css">
    
    <style>
    
        #studentbutton {
            border-bottom: 2px solid rgb(9, 124, 226);
        }

      
    </style>
</head>

<body>
    <div id="nn123">
            <%- include('../partials/navbar') %>
    </div>
    <form action="/login" method="post" enctype="multipart/form-data" onsubmit=""> 
        <div class="login">
            <div class="inner-box">
                <div class="buttons">
                    <div id='studentbutton'>
                        <button ><a href="/login/new">Student</a></button>
                    </div>
                    <div id='companybutton'>
                    <button ><a href="/login/company/new">Company</a></button>
                    </div>
                </div>
                <div>
                    <h2 id='loginh2'>Login</h2>
        
                </div>
                <div class='loginpara'>
                    <p>If you are a company looking to hire interns, please login as a company by clicking <a
                            href="/login/company/new">here</a>
                    </p>
                </div>
                <div class="fbGoogle bg-icon-color1">
                    <a href="">
                        <span>
                            <img id='iconImg' class="iconImg"
                                src="https://www.interntheory.com/assets/iconsLogos/facebook-circular-logo.svg" height="25">
                            <span id='span' style="margin-left: 20%;">SIGNIN WITH FACEBOOK</span>
                        </span>
                    </a>
                </div>
                <div id='gooleicon' class="fbGoogle bg-icon-color2">
                    <a href="">
                        <span>
                            <img class="iconImg" src="https://www.interntheory.com/assets/iconsLogos/googlelogo.svg"
                                height="25">
                            <span style="margin-left: 20%;">SIGNIN WITH GOOGLE</span>
                        </span>
                    </a>
                </div>
                <div class="or">
                    <h5>OR</h5>
                </div>
                <div>
                    <form class="form"action="/login" method="post" enctype="multipart/form-data">
                        <!-- <label for="email">Email/Number</label> -->
                        <input class="loginInputs" aria-invalid="false" type="text"  name="email" placeholder="Email/Number*" id="email" required />
                        <!-- <label for="password">Password</label> -->
                        <input class="loginInputs" type="password" placeholder="Password*" name="password" id="password" required />
                        <div class="not_account">
                        <p>
                            Don't have an account?
                            <span><a href="/register/new">Create your account</a></span>
                            <!-- <span class="color_blue" onclick="create_account()">Create your account</span> -->
                        </p>
                    </div>
                    <button  type="submit" id="login_btn" class="login_btn">LOGIN</button>
                    </form>
                </div>
              
            </div>
        </div>`
    </form>
    <div id="loginformstr"></div>

    <div id="footer"></div>
</body>

</html>

